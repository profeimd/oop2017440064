import java.util.Scanner;

public class Six {

	public static void main(String[] args) {
		// 6번
		System.out.println("6번");
		
		int a, b, c;
		
		System.out.print("두 자리 정수를 입력하세요: ");
		Scanner input = new Scanner(System.in);
		a = input.nextInt();
		b = a/10;
		c = a%10;
		
		if(b==c)
			System.out.println(a);
		else
			System.out.println(c*10+b);
		

	}

}
